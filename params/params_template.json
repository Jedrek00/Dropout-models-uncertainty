{
    "model_architecture": "convnet",
    "random_seed": 100,
    "dataset_name": "fashion_mnist",
    "device": "cuda:0",
    "batch_size": 32,
    "num_of_classes": 10,
    "optimizer": "Adam",
    "lr": 0.0001,
    "epochs": 5,
    "dropout_rate": 0.25,
    "dropout_type": "standard",
    "densenet":
    {
        "layers": [512, 256, 128]
    },
    "convnet":
    {
        "filters": [32, 64, 128],
        "kerenel_sizes": [[3, 3], [3, 3], [3, 3]]
    }
}